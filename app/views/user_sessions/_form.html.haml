= form_tag user_sessions_path, :method => :post do
  .field
    = label_tag :email 
    %br
    = email_field_tag :email
  .field
    = label_tag :password
    %br
    = password_field_tag :password
  .actions
    = submit_tag "Login"

  = link_to 'Login using Github', auth_at_provider_path(provider: :github)